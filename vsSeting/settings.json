// 将设置放入此文件中以覆盖默认设置
{
  "editor.tabSize": 2,
  "editor.wordWrap": true,
  "editor.snippetSuggestions": "top",
  "[php]": {

  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    // "**/*.js": { "when": "$(basename).ts" },
    "**/*.css": { "when": "$(basename).less" }
  },
  "files.associations": {
    "*.php": "php",
    "*.ts": "javascript",
    "*.less": "less"
  }
}