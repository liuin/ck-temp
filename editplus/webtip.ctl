<script type="text/javascript">
    function resizefalsh(obj,min,max) {
    var windw = $(window).width();
    var windh = $(window).height();
    
    if (windw < min.w) {
      obj.width(min.w);
    }else {
      if (windw > max.w) {
        obj.width(max.w);
      }else {
        obj.width(windw);
      }
    }
    
    if (windh < min.h) {
      obj.height(min.h);
    }else {
      if (windh > max.h) {
        obj.height(max.h);
      }else {
        obj.height(windh);
      }
    }
  }

  resizefalsh($(".flashwrap"),{'w':1150,'h':678},{'w':1680,'h':890});

  $(window).resize(function(){
    resizefalsh($(".flashwrap"),{'w':1150,'h':678},{'w':1680,'h':890});
  })
