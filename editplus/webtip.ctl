      }
    });
    
    nav.insertAfter(obj);

    //显示
    function obj_scroll(obj,w,type,callback) {
      switch (type) {
      case 'scroll':
        
        obj.stop().animate({'left':opts.scrollWidth*w},800,'easeInOutExpo',function  () {
          if (callback) {
            callback();
          }
        });
      break
      case 'fade':
