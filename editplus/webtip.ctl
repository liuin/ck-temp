        obj.css({'left':opts.scrollWidth*w}).hide().fadeIn(function  () {
          if (callback){
            callback();
          }
        });
        */
        
      case 'noanmate':
        
        obj.stop().css({'left':opts.scrollWidth*w});
          if (callback) {
            callback();
          }
      break
      default:
      }
    }

    if (opts.loop == true) {
      opts.showType = 'noanmate';
      nav.find("a").eq(1).trigger('click');
    
      opts.showType = 'scroll';
    }
    
    var contr = {
      "prev": function  () {
        click_prev();
      },
      "next": function  () {
        click_next();
      },
      "scroll": function  (val) {
        wpauto = val;
        if (val == 'false') {
          wpauto = false;
          clearInterval(timecount);
        }
        
      }
      
    }
    
    return contr;
  };  
