      var cityVal = $(this).data('city'); 
      ckSelectArea1.city = cityVal;
      ckSelectArea1.showOut($('.wrapright-city'));
      ckSelectArea1.showOut($('.wrapright-prov'));
      ckSelectArea1.putdata();
    })

    //于收货地址一样
