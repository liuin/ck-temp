      $('#block-site').css('transition','0.5s');
      function zoomInMap (obj) {
        $('#block-site').css('zoom','1').data('zoom','normal');
        var tranx = -(obj.offset().left - ($('.block-site-wrap').offset().left) - $('.block-site-wrap').width()/2);
        var trany = -(obj.offset().top - $('.block-site-wrap').offset().top - $('.block-site-wrap').height()/2);

      
        if (tranx > 0) {
          tranx = 0;
        }
        if (tranx < -(($('#block-site').width() - $('.block-site-wrap').width()))) {
          tranx = -(($('#block-site').width() - $('.block-site-wrap').width()));
        }
        
        if (trany > 0) {
          trany = 0;
        }
        
        if (trany < -(($('#block-site').height() - $('.block-site-wrap').height()))) {
          trany = -(($('#block-site').height() - $('.block-site-wrap').height()));
        }
        

        var style = {
          'transform' : 'translate('+ tranx +'px,'+ trany +'px)',
          'zoom' : '1'
        }

        $('#block-site')[0].offsetWidth;

        $('#block-site').css(style);
       
        var $this = obj;
        setTimeout(
          function(){
            $('#block-site').css('transition','0');
            $('#block-site').css('transform','')
            $('.block-site-wrap').scrollLeft(-tranx);
            $('.block-site-wrap').scrollTop(-trany);
          }, 500);
      }


    })

