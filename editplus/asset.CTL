            }
        } else {

            // ms-attr-class="xxx" vm.xxx="aaa bbb ccc"½«ÔªËØµÄclassNameÉèÖÃÎªaaa bbb ccc
            // ms-attr-class="xxx" vm.xxx=false  Çå¿ÕÔªËØµÄËùÓÐÀàÃû
            // ms-attr-name="yyy"  vm.yyy="ooo" ÎªÔªËØÉèÖÃnameÊôÐÔ
            var toRemove = (val === false) || (val === null) || (val === void 0)
            if (!W3C && propMap[attrName]) { //¾ÉÊ½IEÏÂÐèÒª½øÐÐÃû×ÖÓ³Éä
